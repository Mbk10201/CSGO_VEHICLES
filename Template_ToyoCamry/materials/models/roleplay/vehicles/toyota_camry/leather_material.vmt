VertexLitGeneric
{
	$basetexture "models/roleplay/vehicles/shared/skin"
	$bumpmap "models/roleplay/vehicles/shared/leather_light_bump"

	$detail "models/skyautomotive/shared/leather_light"
	$detailblendfactor 0.2

	$envmap "env_cubemap"
	$envmaptint "[0 0 0]"
	$envmapcontrast 1
	$envmapsaturation 0.1
	$envmapfresnel 1

	$phong 1
	$phongexponenttexture "models/roleplay/vehicles/shared/leather_light"
	$phongfresnelranges "[0.5 0.2 2]"
	$halflambert 1
	$phongboost 0.05

	$rimlight 1
	$rimlightexponent 10
	$rimlightboost 0.2

	$basetexturetransform "center 0.05 0.05 scale 2 2 rotate 0 translate 0 0"

	$nodecal 1

	$colorfix "{35 35 35}"
	$rimmask 1

	Proxies
	{
		Equals
		{
			srcVar1 "$colorfix"
			resultVar "$color"
		}
	}
}