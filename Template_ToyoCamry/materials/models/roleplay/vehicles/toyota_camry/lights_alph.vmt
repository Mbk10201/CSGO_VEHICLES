VertexLitGeneric
{
	$basetexture "models/roleplay/vehicles/toyota_camry/lights"
	$bumpmap "models/roleplay/vehicles/toyota_camry/lights_nm"

	$alphatest 1
	$alphatestreference 0.5

	$envmap "env_cubemap"
	$envmaptint "[0.50 0.50 0.50]"
	$envmapcontrast 1
	$envmapsaturation 0.1
	$envmapfresnel 1

	$phong 1
	$phongfresnelranges "[1 1 1]"
	$phongexponent 50
	$phongboost 2

	$nodecal 1

	$allowalphatocoverage 1
	$colorfix "{255 255 255}"

	Proxies
	{
		Equals
		{
			srcVar1 "$colorfix"
			resultVar "$color"
		}
	}
}