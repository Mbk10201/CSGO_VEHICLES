VertexLitGeneric
{
	$basetexture "models/roleplay/vehicles/ambulance/lights"
	$bumpmap "dev/bump_normal"

	$envmap "env_cubemap"
	$normalmapalphaenvmapmask 1
	$envmaptint "[0.40 0.40 0.40]"

	$phong 1
	$phongfresnelranges "[0.3 0.5 1]"
	$halflambert 1
	$phongexponent 20

	$rimlight 1
	$rimlightexponent 10
	$rimlightboost 0.1

	$nodecal 1

	$blendtintbybasealpha 1

	$colorfix "{255 255 255}"
	$rimmask 1

	Proxies
	{
		Equals
		{
			srcVar1 "$colorfix"
			resultVar "$color"
		}
	}
}