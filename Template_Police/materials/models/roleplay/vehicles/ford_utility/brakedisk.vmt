VertexLitGeneric
{
	$basetexture "models/roleplay/vehicles/shared/chrome"
	$bumpmap "models/roleplay/vehicles/shared/skin_nm"
	$surfaceprop Metal

	$nocull 1

	$envmap "env_cubemap"
	$normalmapalphaenvmapmask 1
	$envmaptint "[0.40 0.40 0.40]"
	$envmapcontrast 1
	$envmapfresnel 1

	$phong 1
	$phongfresnelranges "[0.2 0.2 1]"
	$phongexponent 8
	$phongboost 10

	$nodecal 1

	$blendtintbybasealpha 1

	$colorfix "{255 255 255}"

	Proxies
	{
		Equals
		{
			srcVar1 "$colorfix"
			resultVar "$color"
		}
	}
}