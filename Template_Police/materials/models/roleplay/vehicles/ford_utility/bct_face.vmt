VertexLitGeneric
{
	$basetexture "models/roleplay/vehicles/ford_utility/bct_face"

	$nocull 1

	$envmap "env_cubemap"
	$normalmapalphaenvmapmask 1
	$envmaptint "[0.10 0.10 0.10]"
	$envmapcontrast 1
	$envmapfresnel 1

	$phong 1
	$phongfresnelranges "[1 1 1]"
	$phongexponent 8
	$phongboost 10

	$selfillum 1

	$nodecal 1

	$colorfix "{255 255 255}"

	Proxies
	{
		Equals
		{
			srcVar1 "$colorfix"
			resultVar "$color"
		}
	}
}