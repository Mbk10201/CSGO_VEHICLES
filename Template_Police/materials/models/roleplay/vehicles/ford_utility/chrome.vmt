VertexLitGeneric
{
	$basetexture "models/roleplay/vehicles/shared/white"
	$bumpmap "models/roleplay/vehicles/shared/skin_nm"
	$surfaceprop Metal

	$nocull 1

	$envmap "env_cubemap"
	$normalmapalphaenvmapmask 1
	$envmapcontrast 1
	$envmapfresnel 1

	$phong 1
	$phongfresnelranges "[1 1 1]"
	$phongexponent 2
	$phongboost 10

	$nodecal 1

	$colorfix "{100 100 100}"

	Proxies
	{
		Equals
		{
			srcVar1 "$colorfix"
			resultVar "$color"
		}
	}
}