VertexLitGeneric
{
	$basetexture "models/roleplay/vehicles/ford_utility/blue"
	$bumpmap "models/roleplay/vehicles/shared/skin_nm"

	$envmap "env_cubemap"
	$normalmapalphaenvmapmask 1
	$envmaptint "[0.05 0.05 1]"

	$phong 1
	$phongfresnelranges "[1 1 1]"
	$phongexponent 2
	$phongboost 0.1

	$nodecal 1

	$colorfix "{255 255 255}"

	Proxies
	{
		Equals
		{
			srcVar1 "$colorfix"
			resultVar "$color"
		}
	}
}