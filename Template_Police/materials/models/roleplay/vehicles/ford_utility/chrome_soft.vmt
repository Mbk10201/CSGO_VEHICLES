{
	$basetexture "models/roleplay/vehicles/ford_utility/soft_chrome"
	$bumpmap "models/roleplay/vehicles/shared/skin_nm"

	$nocull 1

	$phong 1
	$phongfresnelranges "[1 1 1]"
	$phongexponent 1
	$phongboost 2

	$nodecal 1

	$colorfix "{255 255 255}"

	Proxies
	{
		Equals
