VertexLitGeneric
{
	$basetexture "models/roleplay/vehicles/ford_utility/blueglass"

	$translucent 1

	$envmap "env_cubemap"
	$normalmapalphaenvmapmask 1
	$envmaptint "[1.30 1.30 1]"

	$phong 1
	$phongfresnelranges "[1 1 1]"
	$phongexponent 75
	$phongboost 10

	$nodecal 1

	$colorfix "{255 255 255}"

	Proxies
	{
		Equals
		{
			srcVar1 "$colorfix"
			resultVar "$color"
		}
	}
}