VertexLitGeneric
{
	$basetexture "models/roleplay/vehicles/shared/bl12"
	$bumpmap "models/roleplay/vehicles/shared/skin_nm"
	$surfaceprop Metal

	$nocull 1

	$envmap "env_cubemap"
	$envmaptint "[0.01 0.01 0.01]"
	$envmapcontrast 0.1

	$phong 1
	$phongfresnelranges "[1 1 1]"
	$phongexponent 256

	$nodecal 1

	$colorfix "{255 255 255}"

	Proxies
	{
		Equals
		{
			srcVar1 "$colorfix"
			resultVar "$color"
		}
	}
}