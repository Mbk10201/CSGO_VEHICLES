"VertexlitGeneric"
{
	"$basetexture" "models/roleplay/vehicles/giulia/logos"
	"$bumpmap" "models/roleplay/vehicles/giulia/ext_logos"
	"$nodecal" "1"

	"$phong" "1" 
	"$phongboost" "2" 
	"$phongexponent" 100
	"$phongfresnelranges" "[1 1 1]" 

    	"$envmap" "env_cubemap"
    	"$envmaptint" "[.02 .02 .02]"
    	"$envmapcontrast" "1"
   	"$envmapsaturation" ".1"
    	"$envmapfresnel" "1"

	"$alphatest" "1"
	"$alphatestreference" "0.5"
	"$allowalphatocoverage" "1"

	"$colorfix" "{200 200 200}"
	"Proxies"
	{
		"Equals"
		{
			srcVar1 $colorfix
			resultVar	$color
		}
	}
}
