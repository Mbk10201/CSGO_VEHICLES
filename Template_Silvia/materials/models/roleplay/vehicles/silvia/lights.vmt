VertexLitGeneric
{
	$basetexture "models/roleplay/vehicles/silvia/lights"
	$bumpmap "models/roleplay/vehicles/silvia/lights_nm"

	$nocull 1

	$envmap "env_cubemap"
	$envmaptint "[0.50 0.50 0.50]"
	$envmapcontrast 1
	$envmapsaturation 0.5
	$envmapfresnel 1

	$phong 1
	$phongfresnelranges "[0.05 0.3 1]"
	$phongexponent 15

	$carpenisenabled 1

	Proxies
	{
		Equals
		{
			srcVar1 "$carpenisenabled"
			resultVar "$color"
		}
	}
}