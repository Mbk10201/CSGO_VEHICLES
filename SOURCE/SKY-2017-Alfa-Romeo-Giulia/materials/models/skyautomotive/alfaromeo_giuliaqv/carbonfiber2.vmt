"VertexlitGeneric"
{
	"$basetexturetransform" "center .1 .1 scale 12 12 rotate 0 translate 0 0"

	"$basetexture" "models/skyautomotive/shared/skin"
	"$bumpmap" "models/skyautomotive/alfaromeo_giuliaqv/carbonfiber2"

	"$blendtintbybasealpha" "1"
  	"$surfaceprop" "metal"
	"$nocull" "1"
	"$nodecal" "1"
	"$normalmapalphaenvmapmask" 1

    	"$envmap" "env_cubemap"
    	"$envmaptint" "[.07 .07 .07]"
    	"$envmapcontrast" "1"
   	"$envmapsaturation" ".1"
    	"$envmapfresnel" "1"

    	"$phong" "1"
    	"$phongexponent" "10"
    	"$phongboost" "0.4"
    	"$phongfresnelranges" "[ 0.2 0.2 0.2]"

	"$colorfix" "{20 20 21}"
	"Proxies"
	{
		"Equals"
		{
			srcVar1 $colorfix
			resultVar	$color
		}
	}
}
