VertexLitGeneric
{
	$basetexture "models/roleplay/vehicles/shared/grill"
	$bumpmap "models/roleplay/vehicles/shared/grill_nrm"

	$translucent 1

	$envmap "env_cubemap"
	$normalmapalphaenvmapmask 1
	$envmaptint "[0.07 0.07 0.07]"

	$phong 1
	$phongfresnelranges "[1 1 1]"
	$halflambert 1
	$phongexponent 50
	$phongboost 0.2

	$rimlight 1
	$rimlightexponent 10
	$rimlightboost 0.1

	$nodecal 1

	$colorfix "{255 255 255}"
	$rimmask 1

	Proxies
	{
		Equals
		{
			srcVar1 "$colorfix"
			resultVar "$color"
		}
	}
}