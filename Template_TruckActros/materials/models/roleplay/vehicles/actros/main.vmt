VertexLitGeneric
{
	$basetexture "models/roleplay/vehicles/actros/main"
	$bumpmap "models/roleplay/vehicles/actros/main_bump"

	$envmap "env_cubemap"
	$normalmapalphaenvmapmask 1
	$envmaptint "[0.01 0.01 0.01]"

	$phong 1
	$phongfresnelranges "[1 1 1]"
	$phongtint "[0.10 0.10 0.10]"
	$phongexponent 10
	$phongboost 3

	$rimlight 1
	$rimlightexponent 5
	$rimlightboost 0.9

	$nodecal 1

	$colorfix "{255 255 255}"

	Proxies
	{
		Equals
		{
			srcVar1 "$colorfix"
			resultVar "$color"
		}
	}
}