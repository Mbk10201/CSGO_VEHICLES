VertexLitGeneric
{
	$basetexture "models/roleplay/vehicles/shared/sf_generic_glass"
	$bumpmap "dev/bump_normal"

	$translucent 1

	$envmap "env_cubemap"
	$normalmapalphaenvmapmask 1
	$envmaptint "[0.30 0.30 0.25]"
	$envmapfresnel 1

	$phong 1
	$phongfresnelranges "[0.1 0.3 0.2]"
	$phongexponent 120
	$phongboost 2

	$rimlight 1
	$rimlightexponent 4
	$rimlightboost 0.1

	$nodecal 1

	$colorfix "{255 255 255}"

	Proxies
	{
		Equals
		{
			srcVar1 "$colorfix"
			resultVar "$color"
		}
	}
}