VertexLitGeneric
{
	$basetexture "models/roleplay/vehicles/shared/black"
	$bumpmap "dev/bump_normal"
	$surfaceprop Metal

	$nocull 1

	$envmap "env_cubemap"
	$normalmapalphaenvmapmask 1
	$envmaptint "[0.10 0.10 0.10]"
	$envmapfresnel 1

	$phong 1
	$phongfresnelranges "[0.2 0.2 1]"
	$phongexponent 2
	$phongboost 0.2

	$nodecal 1

	$colorfix "{255 255 255}"

	Proxies
	{
		Equals
		{
			srcVar1 "$colorfix"
			resultVar "$color"
		}
	}
}