VertexLitGeneric
{
	$basetexture "models/roleplay/vehicles/shared/glass_red2"
	$bumpmap "dev/bump_normal"

	$translucent 1

	$envmap "env_cubemap"
	$normalmapalphaenvmapmask 1
	$envmaptint "[1 0.30 0.25]"
	$envmapfresnel 1

	$phong 1
	$phongfresnelranges "[1 2 4]"
	$phongexponent 300
	$phongboost 2

	$rimlight 1
	$rimlightexponent 4
	$rimlightboost 1

	$basetexturetransform "center 0 0 scale 70 70 rotate 0 translate 0 0"

	$nodecal 1

	$colorfix "{255 0 0}"

	Proxies
	{
		Equals
		{
			srcVar1 "$colorfix"
			resultVar "$color"
		}
	}
}