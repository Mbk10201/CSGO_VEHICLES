VertexLitGeneric
{
	$basetexture "dev/bump"
	$bumpmap "models/roleplay/vehicles/shared/skin_paintflakes_bump"

	$envmap "env_cubemap"
	$normalmapalphaenvmapmask 1
	$envmaptint "[0.04 0.04 0.04]"

	$phong 1
	$phongfresnelranges "[2 0.5 1]"
	$phongtint "[0.10 0.20 0.50]"
	$halflambert 1
	$phongexponent 3
	$phongboost 0.08

	$rimlight 1
	$rimlightexponent 1
	$rimlightboost 0.1

	$basetexturetransform "center 0 0 scale 70 70 rotate 0 translate 0 0"

	$nodecal 1

	$colorfix "{10 10 10}"
	$rimmask 1

	Proxies
	{
		Equals
		{
			srcVar1 "$colorfix"
			resultVar "$color"
		}
	}
}