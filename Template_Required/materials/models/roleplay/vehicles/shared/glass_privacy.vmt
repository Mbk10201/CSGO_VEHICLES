VertexLitGeneric
{
	$basetexture "models/roleplay/vehicles/shared/glass_privacy"
	$bumpmap "dev/bump_normal"

	$translucent 1

	$envmap "env_cubemap"
	$normalmapalphaenvmapmask 1
	$envmaptint "[0.20 0.20 0.18]"

	$phong 1
	$phongfresnelranges "[1 1 1]"
	$phongexponent 60
	$phongboost 0.7

	$nodecal 1

	$colorfix "{255 255 255}"

	Proxies
	{
		Equals
		{
			srcVar1 "$colorfix"
			resultVar "$color"
		}
	}
}